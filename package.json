{
  "name": "lookbook-admin",
  "version": "1.0.0",
  "private": true,
  "description": "Admin system for Lookbook photo-first restaurant menus",
  "scripts": {
    "dev:api": "pnpm --filter @lookbook/api dev",
    "dev:ui": "pnpm --filter @lookbook/admin-ui dev",
    "build:api": "pnpm --filter @lookbook/api build",
    "build:ui": "pnpm --filter @lookbook/admin-ui build",
    "deploy:api": "pnpm --filter @lookbook/api deploy",
    "deploy:ui": "pnpm --filter @lookbook/admin-ui deploy",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
